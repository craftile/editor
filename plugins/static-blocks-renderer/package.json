{
  "name": "@craftile/plugin-static-blocks-renderer",
  "version": "0.1.0",
  "description": "Craftile plugin for static HTML block rendering",
  "keywords": [
    "craftile",
    "plugin",
    "static",
    "rendering",
    "blocks"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eldo Magan",
      "email": "magan.eldo@gmail.com"
    }
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build"
  },
  "dependencies": {
    "@craftile/core": "workspace:*",
    "@craftile/editor": "workspace:*",
    "@craftile/preview-client-html": "workspace:*",
    "@craftile/types": "workspace:*",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
